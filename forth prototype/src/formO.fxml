<?import javafx.geometry.Insets?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FormOMenu">
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <TableView fx:id="historyTable" minHeight="-Infinity" prefHeight="311.0" prefWidth="528.0" VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="orderNoColumn" minWidth="79.0" prefWidth="134.0" text="Order Number" />
                <TableColumn fx:id="productCodeColumn" minWidth="200.0" prefWidth="238.0" text="Product Code" />
                  <TableColumn fx:id="kitPriceColumn" minWidth="12.0" prefWidth="163.0" text="Kit Price" />
              </columns>
            </TableView>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Region prefHeight="25.0" prefWidth="377.0" />
                  <Label prefHeight="17.0" prefWidth="154.0" text="Total:" />
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
   <top>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="button" mnemonicParsing="false" onAction="#goToFormV" prefHeight="37.0" prefWidth="99.0" text="Back" textAlignment="CENTER">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </top>
   <left>
      <Region prefHeight="310.0" prefWidth="32.0" BorderPane.alignment="CENTER" />
   </left>
   <bottom>
      <Region prefHeight="32.0" prefWidth="600.0" BorderPane.alignment="BOTTOM_CENTER" />
   </bottom>
   <right>
      <Region prefHeight="310.0" prefWidth="32.0" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
