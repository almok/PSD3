<BorderPane prefHeight="400.0" prefWidth="630.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FormOMenu">
   <center>
   		<HBox alignment="TOP_CENTER" prefHeight="336.0" prefWidth="564.0">
      <children>
   	      <VBox prefHeight="336.0" prefWidth="540.0">
   	         <children>
   	            <TableView fx:id="historyTable" minHeight="-Infinity" prefHeight="311.0" VBox.vgrow="ALWAYS">
   	              <columns>
   	                <TableColumn fx:id="orderNoColumn" minWidth="79.0" prefWidth="135.0" text="Order Number" />
   	                <TableColumn fx:id="productCodeColumn" minWidth="178.0" prefWidth="245.0" text="Product Code" />
