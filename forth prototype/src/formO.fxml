   	              </columns>
   	            </TableView>
   	            <HBox prefHeight="100.0" prefWidth="200.0">
   	               <children>
   	                  <Region prefHeight="25.0" prefWidth="307.0" />
   	                  <Label prefHeight="17.0" prefWidth="91.0" text="Total:" />
   	                  <TextField fx:id="totalKitPrice" editable="false" prefHeight="25.0" prefWidth="153.0" />
   	               </children>
   	            </HBox>
   	         </children>
   	      </VBox>
      </children>
		</HBox>
   </center>
   <top>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="button" mnemonicParsing="false" onAction="#goToFormV" prefHeight="37.0" prefWidth="99.0" text="Back" textAlignment="CENTER">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </top>
   <left>
      <Region prefHeight="310.0" prefWidth="32.0" BorderPane.alignment="CENTER" />
   </left>
   <bottom>
      <Region prefHeight="32.0" prefWidth="600.0" BorderPane.alignment="BOTTOM_CENTER" />
   </bottom>
   <right>
      <Region prefHeight="310.0" prefWidth="32.0" BorderPane.alignment="CENTER" />
   </right>
