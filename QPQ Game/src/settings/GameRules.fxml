<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>

<VBox fx:id="aPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="settings.GameRules">
   <children>
   	<HBox fx:id="topButtons" prefHeight="100.0" prefWidth="600.0">
      <children>
      		<Button fx:id="backButton" layoutX="140.0" layoutY="51.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="99.0" text="Back">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
      		<Button fx:id="saveButton" layoutX="140.0" layoutY="51.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="99.0" text="Save">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
      </children>
   	</HBox>
      <HBox alignment="TOP_CENTER">
         <children>
         	<VBox fx:id="products" alignment="TOP_CENTER" prefWidth="600.0">
            <children>
            		<HBox prefHeight="50.0" prefWidth="600.0">
               <children>
                   			 <Label prefHeight="50.0" prefWidth="150.0" text="Item:" />
                   			 <Label prefHeight="50.0" prefWidth="150.0" text="Amount:" />
               </children>	
            		</HBox>
            </children>
         	</VBox>
         </children>
      </HBox>
   </children>
</VBox>
