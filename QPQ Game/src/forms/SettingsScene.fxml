<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="settings.SettingsScene">
   <children>
      <HBox>
         <children>
         	<Button fx:id="backButton" mnemonicParsing="false" prefHeight="37.0" prefWidth="99.0" text="Back">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox alignment="TOP_CENTER">
         <children>
            <VBox fx:id="aPane" prefHeight="400.0" prefWidth="600.0">
               <children>
                  <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                     	<Button fx:id="gameRulesButton" mnemonicParsing="false" prefHeight="45.0" prefWidth="111.0" text="Game Rules">
                           <HBox.margin>
                              <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
                  <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                     	<Button fx:id="formCButton" mnemonicParsing="false" prefHeight="45.0" prefWidth="111.0" text="Form C">
                           <HBox.margin>
                              <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
                           </HBox.margin>
                        </Button>
                     	<Button fx:id="formDButton" mnemonicParsing="false" prefHeight="45.0" prefWidth="111.0" text="Form D">
                           <HBox.margin>
                              <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
                  <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                     	<Button fx:id="formPButton" mnemonicParsing="false" prefHeight="45.0" prefWidth="111.0" text="Form P">
                           <HBox.margin>
                              <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
                           </HBox.margin>
                        </Button>	
                     	<Button fx:id="formRButton" mnemonicParsing="false" prefHeight="45.0" prefWidth="111.0" text="Form R">
                           <HBox.margin>
                              <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
